{
    "version": 2,
    "builds": [{
        "src": "app.js",
        "use": "@vercel/node"
    }],
    "routes": [{
        "src": "/(.*)",
        "dest": "app.js",
        "methods": ["GET", "OPTIONS", "POST", "PUT", "DELETE"],
        "headers": {
            "Access-Control-Allow-Methods": ["GET", "POST", "OPTIONS", "PUT", "DELETE"],
            "Access-Control-Allow-Headers": "Content-Type"
        }
    }]
}